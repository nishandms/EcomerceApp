<div class="container">
  <div class="col-md-12 p-1">
 <a href="/admin/add-product" class="btn btn-primary float-end">Add Product</a>
  </div>
   
    <div class="col-md-12">
    <table class="table">
  <thead>
    <tr>
      <th scope="col">Poroduct Name</th>
      <th scope="col">Prize</th>
      <th scope="col">Ation</th>
    </tr>
  </thead>
  <tbody>
    {{#each product}}
    <tr>
      <td>{{this.product}}</td>
      <td>{{this.prize}}</td>
      <td><button class="btn btn-danger" onclick="deleteData('{{this._id}}')">Delete</button></td>
    </tr>
    {{/each }}
  </tbody>
</table>
</div>
</div>
